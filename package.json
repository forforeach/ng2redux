  "bugs": {
    "url": ""
  },
  "main": "angular2-redux.js",
  "typings": "./angular2-redux.d.ts",
  "homepage": "",
  "bin": {
    "tsc": "./tsc"
  },
  "devDependencies": {
    "angular2": "^2.0.0-beta.13",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-typescript-preprocessor": "0.0.21",
    "npm-run": "^3.0.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "systemjs": "^0.19.25",
    "systemjs-builder": "^0.15.14",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "zone.js": "^0.6.8"
