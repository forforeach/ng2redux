{
  "name": "angular2-redux",
  "description": "An implementation of Redux for Angular 2",
  "version": "0.0.1",
  "scripts": {
    "test": "npm run tsc && karma start",
    "test-watch": "npm run tsc && karma start --no-single-run --auto-watch",
